{
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "pretty": true,
    "lib": [
      "DOM",
      "ES2019"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/modules/*"
      ]
    }
  },
}
